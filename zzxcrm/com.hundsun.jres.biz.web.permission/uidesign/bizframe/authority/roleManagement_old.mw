function getAllSelectedNodes(menuTree,publicTree){
     var nodeStrs="";
     var menuTreeNodes=menuTree.getAllChildren();
     for(var i=0;i<menuTreeNodes.length;i++){
         var node=menuTreeNodes[i];
	     var nodeStr=getNodeService(node)+'|'+node.attributes.checked+'|'+node.isLeaf()+'|true'
	     nodeStrs=nodeStrs+nodeStr;
	     if(i<menuTreeNodes.length-1){
	     	nodeStrs=nodeStrs+",";
	     }
     }
     var publicTreeNodes=publicTree.getAllChildren();
     if(publicTreeNodes.length>0){
     	nodeStrs=nodeStrs+",";
     }
     for(var j=0;j<publicTreeNodes.length;j++){
         var node=publicTreeNodes[j];
	     var nodeStr=getNodeService(node)+'|'+node.attributes.checked+'|'+node.isLeaf()+'|flase'
	     nodeStrs=nodeStrs+nodeStr;
	     if(j<publicTreeNodes.length-1){
	     	nodeStrs=nodeStrs+",";
	     }
     }
     return nodeStrs;
}

private function getNodeService(node){
 		var path=node.getPath();
 		var paths=path.split("/");
 		if(paths[paths.length-1].indexOf("$")>0){
 			return  paths[paths.length-1]
 		}
 		return paths[paths.length-2]+"$"+ paths[paths.length-1]
}

