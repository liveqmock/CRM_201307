                      <Bottom/>
                      <Left/>
                      <Right/>
                      <layout-table columns="3" fullWidth="true">
                        <control-TextField name="serviceId" fieldLabel="服务ID" labelAlign="right" width="250" maxLength="20" height="22"/>
                        <control-TextField name="serviceType" fieldLabel="服务类型" labelAlign="right" width="250" maxLength="20" height="22"/>
