                        <control-GridItem align="left" width="100" sortable="true" title="服务状态" name="GridItem7"/>
                      </layout-GridToolbar>
                      <Top/>
                      <Bottom/>
                      <Left/>
                      <Right/>
                    </control-Grid>
                  </layout-border>
                </control-Panel>
              </layout-tab>
              <Top/>
              <Bottom/>
              <Left/>
              <Right/>
            </control-TabPanel>
          </layout-fit>
        </control-Panel>
      </layout-fit>
    </control-MainWindow>
